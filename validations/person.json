            "type": "date",
            "patterns": [
                {
                    "max": [
                        {
                            "type": "depending_formula",
                            "value": {
                                "depending": {
                                    "type": "now",
                                    "value": null
                                },
                                "operation": "subtract",
                                "value": 18,
                                "unit": "year"
                            }
                        }
                    ],
                    "min": [
                        {
                            "type": "date",
                            "value": "1923-01-01"
                        }
                    ]
                }
            ]
        },
        {
            "field": "russian_internal_passport",
            "type": "string",
            "allow_white_spaces": true,
            "patterns": [
