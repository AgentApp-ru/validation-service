                    ]
                }
            ]
        },
        {
            "field": "foreign_external_passport",
            "allow_white_spaces": false,
            "patterns": [
                {
                    "name": "series",
                    "patterns": [
                        {
                            "chars": "[A-Z\\d]",
                            "max": 10,
                            "min": 1
                        }
                    ]
                },
