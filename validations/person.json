            "patterns": [
                {
                    "max": [
                        {
                            "type": "now",
                            "value": null
                        }
                    ],
                    "min": [
                        {
                            "type": "depending_formula",
                            "value": {
                                "depending": {
                                    "type": "depending",
                                    "value": {
                                        "scope": "person",
                                        "key": "birth_date"
                                    }
                                },
                                "operation": "add",
                                "value": 14,
                                "unit": "year"
                            }
                        }
                    ]
                }
            ],
            "type": "date"
        },
